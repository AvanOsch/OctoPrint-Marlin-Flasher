<div id="marlin_flasher_modal" class="modal modal-lg hide fade">
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
        <h3>{{ _('Error Output') }}</h3>
    </div>
    <div class="modal-body">
        <p>
            {{ _('The process gave the following error output:')}}
        </p>
        <pre class="scrollable-pre" data-bind="text: stderr"></pre>
    </div>
    <div class="modal-footer">
        <a href="#" class="btn" data-dismiss="modal" aria-hidden="true">{{ _('Close') }}</a>
    </div>
</div>
